{
  "name": "generator-esnext-generator",
  "version": "0.0.1",
  "description": "Generate a Yeoman generator using what's next in JavaScript",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf generators coverage",
    "lint": "eslint . --max-warnings 0",
    "test": "mocha test/ --recursive --compilers js:babel-core/register --reporter spec",
    "test:cover": "babel-node $(npm bin)/isparta cover --reporter text --reporter html $(npm bin)/_mocha test/ -- --recursive --reporter spec",
    "test:watch": "npm test -- --watch --reporter min",
    "check": "npm run lint && npm run test",
    "copy": "copyfiles src/**/!{js} generators/",
    "build:lib": "babel src --out-dir generators",
    "build": "npm run clean && npm run copy && npm run build:lib",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "= <=>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "copyfiles": "^0.2.1",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-config-shopify": "^5.7.1",
    "eslint-plugin-react": "^3.13.1",
    "eslint-plugin-shopify": "^2.0.1",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "mockery": "^1.4.0",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "yeoman-assert": "^2.1.1",
    "yeoman-test": "^1.0.0"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "deep-extend": "^0.4.0",
    "inquirer-npm-name": "^1.0.0",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^0.22.2",
    "yosay": "^1.1.0"
  }
}
